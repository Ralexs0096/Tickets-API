    "knex": "^2.4.2",
    "objection": "^3.0.1",
    "tedious": "^16.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "pino-pretty": "^11.2.1",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
